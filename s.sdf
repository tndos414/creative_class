<?xml version='1.0'?>
<sdf version='1.6'>
  <model name='ladder'>
    <link name="rack">
        <self_collide>1</self_collide>
        <inertial>
            <mass>2.915</mass>
        </inertial>
        <visual name="visual">
            <geometry>
                <mesh>
                    <uri>model://fast_ver2/meshes/collision/rack1.dae</uri>
                    <scale>1 1 1</scale>
                </mesh>
            </geometry>
        </visual>
        <collision name='collision'>
            <geometry>
                <mesh>
                    <uri>model://echo_bot/meshes/STL/rack1.stl</uri>
                    <scale>1 1 1</scale>
                </mesh>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>0.05</mu>
                        <mu2>0.05</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </link>

    <joint name="body_to_fixed_top" type="revolute">
        <pose>0 0.05 0.045 0 0 0</pose>
        <parent>shaft</parent>
        <child>pinion</child>
        <axis>
            <xyz>1 0 0</xyz>
        </axis>
    </joint>
    
    <link name="pinion">
        <self_collide>1</self_collide>
        <inertial>
            <mass>2.915</mass>
        </inertial>
        <visual name="visual">
            <geometry>
                <mesh>
                    <uri>model://fast_ver2/meshes/collision/pinion.dae</uri>
                    <scale>1 1 1</scale>
                </mesh>
            </geometry>
        </visual>
        <collision name='collision'>
            <geometry>
                <mesh>
                    <uri>model://echo_bot/meshes/STL/pinion.stl</uri>
                    <scale>1 1 1</scale>
                </mesh>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>0.05</mu>
                        <mu2>0.05</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </link>

    <link name="shaft_pinion_bevel">
        <self_collide>1</self_collide>
        <inertial>
            <mass>2.915</mass>
        </inertial>
        <visual name="visual">
            <geometry>
                <mesh>
                    <uri>model://fast_ver2/meshes/collision/shaft_pinion_bevel.dae</uri>
                    <scale>1 1 1</scale>
                </mesh>
            </geometry>
        </visual>
        <collision name='collision'>
            <geometry>
                <mesh>
                    <uri>model://echo_bot/meshes/STL/shaft_pinion_bevel.stl</uri>
                    <scale>1 1 1</scale>
